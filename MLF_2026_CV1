        "output_binary = ''\n",
        "\n",
        "while True:\n",
        "    try:\n",
        "        input_integer = int(input(\"Enter an integer to convert it to binary: \"))\n",
        "        break\n",
        "    except ValueError:\n",
        "        print(\"Invalid input! Please enter a valid integer.\")\n",
        "\n",
        "while input_integer > 0:\n",
        "    output_binary = str(input_integer & 1) + output_binary\n",
        "    input_integer >>= 1\n",
        "\n",
        "print(\"Entered integer is \" + str(output_binary) + \" in binary\")"
